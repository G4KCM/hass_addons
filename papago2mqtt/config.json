{
    "name": "Papago 2 MQTT",
    "version": "1.1",
    "slug": "papago2mqtt",
    "description": "Homeassistant interface with Papouch Papago Sensors",
    "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
    "startup": "application",
	"services": ["mqtt:need"],
    "boot": "auto",
    "hassio_api": true,
    "options": {
        "papagoip":"192.168.1.1",
        "refreshseconds":"60"
    },
    "schema": {
        "papagoip":"str",
        "refreshseconds":"int"
	}
}